{
    "source": {
        "include": ["./newroute.js"],
        "includepattern": ".js$",
        "excludepattern": "(node_modules/|docs)"
    },
    
    "plugins": [
        "plugins/markdown"
    ],
    
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true

    }
}